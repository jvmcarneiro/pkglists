# Retrieve packages explicitly installed through pacman
pacman -Qqe | grep -v "$(pacman -Qqm)" > pacman.lst

# Retrieve foreign packages (usually AUR)
pacman -Qqm > aur.lst

# Install pacman.lst
pacman -S --needed $(cat pacman.lst)

# Install aur.lst
yaourt -S $(cat aur.lst)

# Git repos can be listed and updated with the myrepos tool
